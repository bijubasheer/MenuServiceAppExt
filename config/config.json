{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "jmf-dd-menu-ext-app-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/jmfDdMenuExtApp/JmfDdMenuExtAppApplicationCustomizer.js",
          "manifest": "./src/extensions/jmfDdMenuExtApp/JmfDdMenuExtAppApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js",
      "globalName": "jQuery"
   },
   "myScript":{
